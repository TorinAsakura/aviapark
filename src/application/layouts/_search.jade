block search
   #search.search.search--main.js-search--main.is-hidden
       input.search__text-field.js-search__text-field(type='text', placeholder='Поиск')
       .search-drop.js-search-drop.is-hidden
           .search-drop__char-title А
           .search-drop__item
               .search-drop__brand-img-container
                   img.search-drop__brand-img(src='/img/logos/200_01.jpg', alt='')
